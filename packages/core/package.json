{
  "name": "@r2s/core",
  "version": "1.0.0",
  "description": "react to svelte core",
  "author": "Albert Zhang (https://github.com/AlbertAZ1992)",
  "homepage": "",
  "license": "ISC",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "tslint  --project tsconfig.json 'src/**/*.ts' 'docs/**/*.ts' 'docs/**/*.tsx'"
    ]
  },
  "scripts": {
    "lint": "tslint  --project tsconfig.json 'src/**/*.ts' 'docs/**/*.ts' 'docs/**/*.tsx'",
    "build": "rm -rf lib && tsc --target es2015 --declaration && cp -r ./output/src/. ./lib && rm -rf output"
  },
  "dependencies": {
    "@babel/core": "^7.14.6",
    "@babel/parser": "^7.14.6",
    "@babel/template": "^7.14.5",
    "@babel/types": "^7.14.5",
    "ast-types": "^0.14.2",
    "lodash": "^4.17.21",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/babel-generator": "^6.25.4",
    "@types/babel-template": "^6.25.2",
    "@types/babel-traverse": "^6.25.7",
    "@types/babel-types": "^7.0.11",
    "@types/node": "^16.7.2",
    "@types/rimraf": "^3.0.2",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.4.5"
  },
  "files": [
    "lib"
  ]
}
